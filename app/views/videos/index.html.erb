<h1>Listing videos</h1>

<table>
  <tr>
  </tr>

<% @videos.each do |video| %>
  <tr>
    <td><%= image_tag(video.media.url(:thumbnail))%> <strong><%= link_to video.title, video %></strong> >> <%= link_to 'Destroy', video, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New video', new_video_path %>